net: "models/LeNet/lenet_train.prototxt"

test_iter: 100
test_interval: 500

type: "SGD"
base_lr: 0.01
momentum: 0.9
weight_decay: 0.0005
lr_policy: "inv"
gamma: 0.0001
power: 0.75

debug_info: 1
test_compute_loss: 1


display: 100
max_iter: 10000

snapshot: 5000
snapshot_prefix: "models/LeNet/states/lenet"

solver_mode: CPU
